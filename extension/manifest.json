{
  "name": "CastList",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Automatically tags names in Times news articles and provides biographical information.",
  "background": {
    "scripts": [ "go.js" ]
  },
  "permissions": [
    "activeTab",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "CastList"
  }
}
